{
  "manifest_version": 2,

  "name": "Gradescoper",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "js": ["index2.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'"
}