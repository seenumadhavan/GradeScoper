{
  "manifest_version": 2,

  "name": "Gradescoper",
  "content_scripts": [{
    "js": ["index2.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "version": "1.0",

  "browser_action": {
    "default_icon": "gs.jpg",
    "default_popup": "index2.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'"
}